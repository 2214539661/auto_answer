{
  "manifest_version": 2,
  "name": "测评量表自动答题脚本1.3",
  "version": "1.3",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js",
        "popup.js"
      ],
      "content_security_policy": "script-src 'self' 'nonce-random_nonce_value' 'unsafe-inline' blob: filesystem:;"
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}